Algoritmo "ExercicioDeposito"

Var nome: caractere
   saldo1, deposito, saldo2: real

Inicio
   escreva("Digite seu nome: ")
   leia(nome)
   escreval("Saldo Inicial: ")
   leia(saldo1)
   escreval("Valor do deposito: ")
   leia(deposito)

   escreval("Saldo inicial: ", (saldo1))
   escreval("Deposito: ", (deposito))
   escreval("Saldo Atual", (saldo1+deposito))
Fimalgoritmo